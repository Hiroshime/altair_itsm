<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>altair2018-web</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <description/>
        <display-name>Servlet</display-name>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>altair2018.control.web.Servlet</servlet-class>
    </servlet>
    
    
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/userPage/AddCart</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/altair_page_bootstrap/pages/Login</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/altair_page_bootstrap/pages/BaselineGeral</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/admPage/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/admnPage/pages/ReportCatMon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/userPage/LoginOrder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/userPage/AddressOrder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/userPage/ShipAddress</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/userPage/PaymentOrder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/admnPage/pages/Product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/RelatorioEntrada</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
